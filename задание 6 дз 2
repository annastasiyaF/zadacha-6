#include <stdio.h>

int main(){
    int a;
    int b;
    int c;
    int d;
    scanf("%d %d %d %d", &a, &b, &c, &d);
    if (a == b == c){
        printf("4");
    }
    if (a == b == d){
        printf("3");
    }
    if (a == c == d){
        printf("2");
    }
    if (b == c == d){
        printf("1");
    }
    return 0;
}
